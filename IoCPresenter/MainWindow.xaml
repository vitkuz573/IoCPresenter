<Window x:Class="IoCPresenter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IoCPresenter"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        
        <Button Content="Show IoC Services" Click="ShowIoCServicesButton_Click" Grid.Row="0" Margin="10" />
        <Button Content="Get Message from Service" Click="GetMessageFromServiceButton_Click" Grid.Row="1" Margin="10" />
    </Grid>
</Window>
